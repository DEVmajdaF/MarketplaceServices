
@model IEnumerable<MarketplaceServices.ViewModel.UserRoleViewModel>;

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Email</th>
            <th scope="col">RoleName</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {

        <tr>
            <th scope="row">@item.Id</th>
            <td>@item.Email</td>
            @if(@item.Id.Count() > 1 ){
            <td>@item.RoleName</td>
            }



        </tr>

        }

    </tbody>
</table>
